<div class="content p-1">
    <div class="list-group-item">
        <div class="d-flex">
            <div class="mr-auto p-2">
                <h2 class="display-4 titulo">Conteúdo da página Contato</h2>
            </div>

            <div class="p-2">
                <span class="d-none d-md-block">
                    <a href="/contato/edit-contato" class="btn btn-outline-warning btn-sm">Editar</a>
                    <a href="/contato/edit-contato-img" class="btn btn-outline-warning btn-sm">Editar Imagem</a>
                    
                </span>
                
                <div class="dropdown d-block d-md-none">
                    <button class="btn btn-primary dropdown-toggle btn-sm" type="button" id="acoesListar"
                        data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Ações
                    </button>
                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="acoesListar">
                         <a class="dropdown-item" href="/contato/edit-contato">Editar </a>
                        
                    </div>

                </div>
            </div>
        </div>
        {{>_msg}}
        <hr>
        
        <dl class="row">
            
            <dt class="col-sm-3">ID</dt>
            <dd class="col-sm-9">{{contatopg._id}}</dd>

            <dt class="col-sm-3">Endereço  </dt>
            <dd class="col-sm-9">{{contatopg.endereco}}</dd>

            <dt class="col-sm-3">Telefone</dt>
            <dd class="col-sm-9">{{contatopg.telefone}} </dd>

            <dt class="col-sm-3">Email</dt>
            <dd class="col-sm-9">{{contatopg.email}}</dd>

            <dt class="col-sm-3">Site</dt>
            <dd class="col-sm-9">{{contatopg.site}}</dd>

           
       <hr>
        
    </div>
</div>