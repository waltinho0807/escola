<div class="content p-1">
    <div class="list-group-item">
        <div class="d-flex">
            <div class="mr-auto p-2">
                <h2 class="display-4 titulo">Conteúdo do slide no topo na home</h2>
            </div>

            <div class="p-2">
                <span class="d-none d-md-block">
                    <a href="/edit-home-top" class="btn btn-outline-warning btn-sm">Editar</a>
                    <a href="/edit-home-top-img1" class="btn btn-outline-warning btn-sm">Editar Img 1</a>
                    <a href="/edit-home-top-img2" class="btn btn-outline-warning btn-sm">Editar Img 2</a>
                </span>
                
                <div class="dropdown d-block d-md-none">
                    <button class="btn btn-primary dropdown-toggle btn-sm" type="button" id="acoesListar"
                        data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Ações
                    </button>
                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="acoesListar">
                        <a class="dropdown-item" href="/edit-home-top">Editar</a>
                        <a class="dropdown-item" href="/edit-home-top-img1">Editar Img 1</a>
                        <a class="dropdown-item" href="/edit-home-top-img2">Editar Img 2</a>
                    </div>

                </div>
            </div>
        </div>
        {{>_msg}}
        <hr>
        <dl class="row">

            <dt class="col-sm-3">ID</dt>
            <dd class="col-sm-9">{{hometopo._id}}</dd>

            <dt class="col-sm-3">Titulo 1</dt>
            <dd class="col-sm-9">{{hometopo.tituloum}}</dd>

            <dt class="col-sm-3">Titulo Botão 1</dt>
            <dd class="col-sm-9">{{hometopo.titulobtnum}} </dd>

            <dt class="col-sm-3">Url Botão 1</dt>
            <dd class="col-sm-9">{{hometopo.urlbtnum}}</dd>

            <dt class="col-sm-3">Titulo 2</dt>
            <dd class="col-sm-9">{{hometopo.titulodois}}</dd>

            <dt class="col-sm-3">Titulo Botão 2</dt>
            <dd class="col-sm-9">{{hometopo.titulobtndois}} </dd>

            <dt class="col-sm-3">Url Botão 2</dt>
            <dd class="col-sm-9">{{hometopo.urlbtndois}}</dd>

            <dt class="col-sm-3">Subtitulo 1 </dt>
            <dd class="col-sm-9">{{hometopo.subtituloum}}</dd>

            <dt class="col-sm-3">Descrição 1</dt>
            <dd class="col-sm-9">{{hometopo.descum}}</dd>

            <dt class="col-sm-3">Subtitulo 2 </dt>
            <dd class="col-sm-9">{{hometopo.subtitulodois}}</dd>

            <dt class="col-sm-3">Descrição 2</dt>
            <dd class="col-sm-9">{{hometopo.descdois}} </dd>

            <dt class="col-sm-3">Subtitulo 3 </dt>
            <dd class="col-sm-9">{{hometopo.subtitulotres}}</dd>

            <dt class="col-sm-3">Descrição 3</dt>
            <dd class="col-sm-9">{{hometopo.desctres}} </dd>

            <dt class="col-sm-3">Subtitulo 4 </dt>
            <dd class="col-sm-9">{{hometopo.subtituloquatro}}</dd>

            <dt class="col-sm-3">Descrição 4</dt>
            <dd class="col-sm-9">{{hometopo.descquatro}} </dd>

            <dt class="col-sm-3">Imagem Slide Topo 1 </dt>
            <dd class="col-sm-9"><img class="img-fluid" src="/images/home/slide_home_top2.jpg"></dd>

            <dt class="col-sm-3">Imagem Slide Topo 2</dt>
            <dd class="col-sm-9"><img class="img-fluid" src="/images/home/slide_home_top1.jpg"></dd>



        </dl>
    </div>
</div>